addr = 0x33099E50

KEY_A  = 1
KEY_DR = 1 << 4
KEY_DL = 1 << 5
KEY_L  = 1 << 9

if check_addr(addr) then
  if is_pressed(KEY_A | KEY_DL) then
    writef(addr, X - 1.0)
  end

  if is_pressed(KEY_A | KEY_DR) then
    writef(addr, X + 1.0)
  end

  if is_pressed(KEY_L) then
    notify("Hello!")
  end
end